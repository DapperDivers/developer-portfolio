{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(jsx|js)$",
    "excludePattern": "(node_modules|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "better-docs/component"
  ],
  "templates": {
    "better-docs": {
      "name": "Developer Portfolio Documentation"
    }
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md"
  }
}
