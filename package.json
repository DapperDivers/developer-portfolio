  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true vite build",
    "serve": "node server.js",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:svgs": "node scripts/optimize-svgs.js",
    "optimize:all": "npm run optimize:images && npm run optimize:svgs",
    "optimize": "npm run optimize:all",
    "generate:metadata": "node scripts/generate-asset-metadata.js",
    "test": "cross-env NODE_ENV=test node --no-warnings node_modules/jest/bin/jest.js --config=jest.config.cjs",
    "test:watch": "cross-env NODE_ENV=test node --no-warnings node_modules/jest/bin/jest.js --config=jest.config.cjs --watch",
    "test:coverage": "cross-env NODE_ENV=test node --no-warnings node_modules/jest/bin/jest.js --config=jest.config.cjs --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-story": "node scripts/generate-story.js",
    "generate-component": "node scripts/generate-component.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "esbuild": "^0.25.0"
  },
